a = int(input())
if ((a%100))%10 == 0 or (a//10)% 10 == 1 or a % 10 == 1 and a % 10 == 2 and a % 10 == 3 and a % 10 == 4 or a % 10 == 5 or a % 10 == 6 or a % 10 == 7 or a % 10 == 8 or a % 10 == 9:
    print(a, "программи" + "стов")
elif a % 10 == 1 and (a//10)% 10 != 1:
    print(a, "программи" + "ст")
elif a% 10 == 2 or a% 10 == 3 or a% 10 == 4 and (a//10)%10 !=1:
    print(a, "программи" + "ста")
