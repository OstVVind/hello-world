a=str(input())
if a[-1] == str(1) or a[-2] != str(1):
    print (a, 'программист')
    
    a=int(input())
if a > 100 or a >= 0:
    a % 100 == 0
    a % 10 == 0
    print (a, 'программист' + 'ов')
    
    a = str(input())
if a[-2] != 1 and a[-1] == str(2) or a[-1] == str(3) or a[-1] == str(4):
    print (a, 'программист' + 'а')
    
    a = str(input())
if a[-1] == str(5) or a[-1] == str(6) or a[-1] == str(7) or a[-1] == str(8) or a[-1] == str(9):
    print (a, 'программист' + 'ов')
